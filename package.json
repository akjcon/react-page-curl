{
  "name": "react-page-curl",
  "version": "0.1.0",
  "description": "A beautiful page curl effect component for React with theme toggle support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "page-curl",
    "theme-toggle",
    "dark-mode",
    "light-mode",
    "ui",
    "component"
  ],
  "author": "Jack Consenstein",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackconsenstein/react-page-curl"
  },
  "homepage": "https://github.com/jackconsenstein/react-page-curl#readme",
  "bugs": {
    "url": "https://github.com/jackconsenstein/react-page-curl/issues"
  }
}

